{
  "AWSTemplateFormatVersion" : "2010-09-09",

  "Description" : "Template Creates new repository for docker image",
  "Parameters": {
    "repositoryName": {
        "Type": "String",
        "Description": "Implements the new repositories name"
    }
  },
  "Resources" : {
    "NewRepo": {
      "Type" : "AWS::ECR::Repository",
      "Properties" : {
          "RepositoryName" : {
            "Ref": "repositoryName"
          },
          "EncryptionConfiguration": {
            "EncryptionType" : "AES256"
          },
          "ImageScanningConfiguration": {
            "ScanOnPush": false
          }
      }
    }
  }
}